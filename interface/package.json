            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.+(js|jsx|json|css|scss|sass|md)": [
            "prettier --write",
            "git add"
        ]
    },
    "devDependencies": {
        "husky": "^4.3.0",
        "lint-staged": "^10.4.2",
        "prettier": "^2.1.2",
